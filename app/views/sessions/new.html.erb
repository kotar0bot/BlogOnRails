<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_with scope: :session, local: true do |form| %>
  <%= form.label :email, 'メールアドレス' %>
  <%= form.text_field :email, class: 'form-control', id: 'session_email' %>

  <%= form.label :password, 'パスワード' %>
  <%= form.text_field :password, class: 'form-control', id: 'session_password' %>

  <%= form.submit 'ログイン', class: 'btn btn-primary' %>
<% end %>